
@{
    ViewData["Title"] = "KeIndex";
}

<h1>KeIndex</h1>
<script src="~/js/vue.min.js"></script>
<script src="~/js/axios.min.js"></script>

<script src="~/layui-v2.5.6/layui/layui.js"></script>
<link href="~/layui-v2.5.6/layui/css/layui.css" rel="stylesheet" />
<div class="layui-form" id="app">
    <table class="layui-table">
        <colgroup>
            <col width="150">
            <col width="150">
            <col width="200">
            <col>
        </colgroup>
        <thead>
            <tr>
                <th>序号</th>
                <th>姓名</th>
                <th>电话</th>
                <th>身份证号</th>
                <th>地址</th>
                <th>贷款状态</th>
                <th>备注</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            <tr v-for="i in emps" v-if=>
                <td>A-00{{i.sId}}</td>
                <td>{{i.sName}}</td>
                <td>{{i.sPhone}}</td>
                <td>{{i.sCard}}</td>
                <td>{{i.sSfrom}}{{i.sXFrom}}</td>
                <td>{{i.sKuan}}</td>
                <td>{{i.sBei}}</td>
                <td><button type="button" class="layui-btn layui-btn-radius" v-on click="Cha()">查看</button></td>
            </tr>
        </tbody>
    </table>
</div>


<script type="text/javascript">
    new Vue({
        el: '#app',
        data: {
            emps: ''
        },
        mounted() {
            axios
                .get('http://localhost:51166/api/User/KeShow')
                .then( response=> (this.emps = response.data))
                .catch(function () {
                    console.log(error);
                });
        }
    })
</script>
